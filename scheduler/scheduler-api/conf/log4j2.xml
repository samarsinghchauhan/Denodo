<?xml version="1.0" encoding="UTF-8"?>

<Configuration>

    <Appenders>

        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%-4r [%t] %-5p %d{yyyy-MM-dd'T'HH:mm:ss.SSS} %c %x - %m  %n" />
        </Console>

    </Appenders>

    <Loggers>

        <Logger name="com.denodo" level="ERROR" />

        <Root level="ERROR">
            <AppenderRef ref="STDOUT" />
        </Root>

    </Loggers>

</Configuration>
